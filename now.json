{
  "routes": [
    {
      "src": "/",
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {
      "src": "/(js|css)/",
      "headers": {
        "cache-control": "public, s-maxage=15552000, max-age=15552000, must-revalidate"
      }
    },
    {
      "src": "/(.*)",
      "rewrite": "/"
    }
  ]
}